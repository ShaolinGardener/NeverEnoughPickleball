<head>

    <!-- Basic Page Needs -->
    <meta charset="utf-8">
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- Mobile Specific Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <!-- Favicons -->
    <link rel="shortcut icon" href="/gsm/content/gsmservice/images/favicon.ico">

    <!-- FONTS -->
    <link rel='stylesheet' href='https://fonts.googleapis.com/css?family=Roboto:100,300,400,400italic,700'>
    <link rel='stylesheet' href='https://fonts.googleapis.com/css?family=Patua+One:100,300,400,400italic,700'>
    <link rel='stylesheet' href='https://fonts.googleapis.com/css?family=Lato:400,400italic,700,700italic,900'>
    <link rel='stylesheet' href='https://fonts.googleapis.com/css?family=Poppins:100,300,400,400italic,500,700,700italic'>
    <style>
        body {
            font-family: 'Poppins', sans-serif;
            color: #555;
            background: #F9F9FA;
            margin: 0;
            padding: 0;
            justify-content: center;
            align-items: center;
            height: 100vh;
        }

        .right-section {
            flex: 1;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            padding: 0 50px;
        }

        .heading {
            text-align: center;
            margin-bottom: 20px;
        }

            .heading h2 {
                font-size: 25px;
                color: #FF7800;
                margin: 0;
            }

            .heading p {
                font-size: 16px;
                color: #424242;
            }

        .input-container {
            width: 100%;
            margin-bottom: 20px;
            
        }

        input, select {
            width: 100%;
            padding: 10px;
            border: 1px solid #000;
            border-radius: 5px;
            box-shadow: 0px 2px 4px rgba(0, 0, 0, 0.044);
        }

        .input-group {
            display: flex;
            gap: 10px;
        }

            .input-group input {
                flex: 1;
            }

        .cta-button {
            width: 100%;
            padding: 10px;
            background: #FF7800;
            color: #FFF;
            border: none;
            border-radius: 5px;
            box-shadow: 0px 4px 13px rgba(0, 0, 0, 0.044);
            font-size: 16px;
            cursor: pointer;
        }

            .cta-button:hover {
                background: #e06600;
            }

        p {
            text-align: center;
        }

            p a {
                color: #FF7800;
                text-decoration: none;
            }

                p a:hover {
                    text-decoration: underline;
                }
    </style>

    <!-- CSS -->
    <link rel='stylesheet' href='/gsm/css/global.css'>
    <link rel='stylesheet' href='/gsm/content/gsmservice/css/structure.css'>
    <link rel='stylesheet' href='/gsm/content/gsmservice/css/gsmservice.css'>
    <link rel='stylesheet' href='/gsm/content/gsmservice/css/custom.css'>
    <link href="~/css/site.css" rel="stylesheet" />
    <!-- Revolution Slider -->
    <link rel="stylesheet" href="/gsm/plugins/rs-plugin-5.3.1/css/settings.css">



    @*<meta name="viewport" content="width=device-width, initial-scale=1.0" />*@
    <title>Never Enough Pickleball - NEP</title>
</head>

@RenderBody()
@await RenderSectionAsync("Scripts", required: false)

</html>