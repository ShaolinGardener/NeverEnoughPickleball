    <script>
        var revapi1, tpj = jQuery;
        tpj(document).ready(function () {
            if (tpj("#rev_slider_1_1").revolution == undefined) {
                revslider_showDoubleJqueryError("#rev_slider_1_1");
            } else {
                revapi1 = tpj("#rev_slider_1_1").show().revolution({
                    sliderType: "hero",
                    sliderLayout: "auto",
                    dottedOverlay: "none",
                    delay: 9000,
                    visibilityLevels: [1240, 1024, 778, 480],
                    gridwidth: 1240,
                    gridheight: 960,
                    lazyType: "none",
                    shadow: 0,
                    spinner: "spinner3",
                    autoHeight: "off",
                    disableProgressBar: "on",
                    hideThumbsOnMobile: "off",
                    hideSliderAtLimit: 0,
                    hideCaptionAtLimit: 0,
                    hideAllCaptionAtLilmit: 0,
                    debugMode: false,
                    fallbacks: {
                        simplifyAll: "off",
                        disableFocusListener: false,
                    }
                });
            }


            tpj(".tech-square").each(function (index, value) {
                tpj(this).hover(function () {
                    //alert("in here");
                    toggleThumbnail(this.dataset.id);
                });

                tpj(this).mouseleave(function () {
                    resetThumbs();
                })
            });
        });



        var currentId = 0;

        function resetThumbs() {
            tpj("#cat-thumb-" + currentId).show();
            tpj("#cat-thumb-hover-" + currentId).hide();
        }

        function toggleThumbnail(id) {
            //debugger;
            resetThumbs();
            currentId = id;
            var img = tpj("#cat-thumb-" + id);
            var imghover = tpj("#cat-thumb-hover-" + id);
            img.hide();
            imghover.show();
        }


        const emailInput = document.getElementById('email');
        const membershipEmailInput = document.getElementById('membershipemail');
        const nameInput = document.getElementById('name');
        const phoneInput = document.getElementById('phone');
        const zipcodeInput = document.getElementById('zipcode');
        const firstNameInput = document.getElementById('firstname');
        const lastNameInput = document.getElementById('lastname');
        emailInput.addEventListener('blur', function () { validateEmail(emailInput); });
        nameInput.addEventListener('blur', function () { validateName(nameInput); });
        phoneInput.addEventListener('blur', validatePhone);
        zipcodeInput.addEventListener('blur',validateZipcode);
        firstNameInput.addEventListener('blur', function () { validateName(firstNameInput); });
        lastNameInput.addEventListener('blur', function () { validateName(lastNameInput); });
        membershipEmailInput.addEventListener('blur', function () { validateEmail(membershipEmailInput); });
        //var emailIsValid = false;
        //var phoneIsValid
        //var nameIsValid = false;
        var formIsValid = false;


        // Function to validate the email address
        function validateEmail(emailObj) {

            const email = emailObj.value.trim();
            const emailRegex = /^[A-Za-z0-9._%+-]+@@[A-Za-z0-9.-]+\.[A-Za-z]{2,}$/;
            if (emailRegex.test(email)) {
                emailObj.classList.remove('error');
                //emailIsValid = true;
                return true;
            } else {
                emailObj.classList.add('error');
                //emailIsValid = false;
                return false;
            }
        }

        // Function to validate the email address
        function validatePhone() {
            const phone = phoneInput.value.trim();
            const phoneRegex = /^\d{10}$/;
            if (phoneRegex.test(phone)) {
                phoneInput.classList.remove('error');
                //emailIsValid = true;
                return true;
            } else {
                phoneInput.classList.add('error');
                //emailIsValid = false;
                return false;
            }
        }

        // Function to validate the email address
        function validateZipcode() {
            const zipcode = zipcodeInput.value.trim();
            const zipcodeRegex = /^\d{5}$/;
            if (zipcodeRegex.test(zipcode)) {
                zipcodeInput.classList.remove('error');
                //emailIsValid = true;
                return true;
            } else {
                zipcodeInput.classList.add('error');
                //emailIsValid = false;
                return false;
            }
        }

        // Function to validate the email address
        function validateName(nameObj) {

            const name = nameObj.value.trim();

            if (name.length > 0) {
                nameObj.classList.remove('error');
                //nameIsValid = true;
                return true;
            } else {
                nameObj.classList.add('error');
                //nameIsValid = false;
                return false;
            }
        }

        function sendContactForm() {
            //debugger;
            formIsValid = validateEmail(emailInput) && validateName(nameInput);
            if (formIsValid) {
                tpj.post('/Contact/Index', {
                    name: tpj('#name').val(), // Replace with your data parameters
                    email: tpj('#email').val(),
                    subject: tpj('#subject').val(),
                    body: tpj('#body').val()
                }, function (response) {
                    // Handle the response from the server
                    console.log(response);
                }).fail(function (jqXHR, textStatus, errorThrown) {
                    // Handle any errors that occurred during the request
                    console.log(textStatus, errorThrown);
                }).always(function () {
                    tpj('#contactform').slideUp("slow").hide();
                    tpj('#contactWrapper').append('<div class="success"><h4>Email Successfully Sent!</h4><br><p>Thank you for using our contact form <strong>' + decodeURIComponent(name) + '</strong>! Your email was successfully sent and we&#39;ll be in touch with you soon.</p></div>');
                });
            }
        }

        function sendMembershipForm() {
            //debugger;
            formIsValid = validateEmail(membershipEmailInput) && validateName(firstNameInput) &&
                validateName(lastNameInput) && validatePhone();
            if (formIsValid) {
                tpj.post('/User/Register', {
                    firstname: tpj('#firstname').val(),
                    lastname: tpj('#lastname').val(),
                    email: tpj('#membershipemail').val(),
                    phone: tpj('#phone').val(),
                    zipcode: tpj('#zipcode').val(),
                    referred: tpj('#referred').val()
                }, function (response) {
                    // Handle the response from the server
                    //debugger;
                    console.log(response);
                }).fail(function (jqXHR, textStatus, errorThrown) {
                    // Handle any errors that occurred during the request
                    console.log(textStatus, errorThrown);
                }).always(function () {
                    tpj('#membershipform').slideUp("slow").hide();
                    tpj('#membershipWrapper').append('<div class="success"><h4>Your account has been created!</h4><br><p>Stay tuned as we add more features <strong>' + decodeURIComponent(name) + '</strong>! </p></div>');
                });
            }
        }

    </script>

