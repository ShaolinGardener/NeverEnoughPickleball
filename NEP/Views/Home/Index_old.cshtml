    <script>
        var obscura = '';

        $(document).ready(function () {
            $("#service-nav").unbind();
            $(".nav-header").click(function (evt) {
                var open = $(".open");
                var id = $(this).data("cat-id");
                $(".nav-li-hidden").hide(500);
                $(".expand-id-" + id).show(500);
                return false;
            });

            $(".nav-footer-header").click(function (evt) {
                var id = $(this).data("footer-cat-id");
                $(".nav-footer-li-hidden").hide(500);
                $(".footer-expand-id-" + id).show(500);
                return false;
            });

            $(".tech-square").each(function (index, value) {
                $(this).hover(function () {
                    toggleThumbnail(this.dataset.id);
                });

                $(this).mouseleave(function () {
                    resetThumbs();
                })
            });

            $(".customer-icon").each(function (index, value) {
                $(this).hover(function () {
                    toggleCustomerThumbnail(this.dataset.id);
                });

                $(this).mouseleave(function () {
                    resetCustomerThumbs();
                })
            });

            if (obscura_h1 != '') {
                //debugger;
                $('#obscura-h1').text(obscura_h1);
            }

            obscura = setTimeout(HideObscura, 10000);
        });

        function HideObscura() {
            //alert('obscura');
            $(".obscura").hide();
        }

        var currentId = 0;

        function toggleThumbnail(id) {
            resetThumbs();
            currentId = id;
            var img = $("#cat-thumb-" + id);
            var imghover = $("#cat-thumb-hover-" + id);
            img.hide();
            imghover.show();
        }

        function resetThumbs() {
            $("#cat-thumb-" + currentId).show();
            $("#cat-thumb-hover-" + currentId).hide();
        }

        // customer toggle section
        var currentCustomerId = 0;
        var divSectionReset = true;

        function toggleCustomerThumbnail(id) {

            //    debugger;
            if (divSectionReset) {
                //resetCustomerThumbs();
                currentCustomerId = id;
                var img = $("#customer-thumb-" + id);
                var mid = $("#customer-thumb-mid-" + id);
                var imghover = $("#customer-thumb-hover-" + id);
                img.hide();
                //mid.show();
                //mid.delay(1000).hide();
                imghover.show();
                //      debugger;

                var divSectionValue = $('#div-section-value-' + id);
                var divSectionId = divSectionValue.data("id");
                var divSection = $('#div-section-id-' + divSectionId);
                divSection
                    .html(divSectionValue.val())
                    .show(500);
                divSectionReset = false;
            }
        }

        function resetCustomerThumbs() {
            if (!divSectionReset) {
                $("#customer-thumb-" + currentCustomerId).show();
                $("#customer-thumb-hover-" + currentCustomerId).hide();
                //$(".div-section-description").hide(1000);
                var id = currentCustomerId;
                var divSectionValue = $('#div-section-value-' + id);
                var divSectionId = divSectionValue.data("id");
                var divSection = $('#div-section-id-' + divSectionId);


                divSection
                    .html('')
                    .hide(500);
                divSectionReset = true;
            }
        }

    </script>
